  "dependencies": {
    "highlight.js": "^11.9.0",
    "marked": "^12.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.31",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  }
