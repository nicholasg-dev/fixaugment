{
  "name": "fix-sugment-vscode",
  "displayName": "Fix Sugment VSCode",
  "description": "Enhanced Augment extension with improved formatting and features (forked from MrXploisLite's fix-augment)",
  "version": "1.1.1",
  "publisher": "WindsurfCustom",
  "author": {
    "name": "Nicholas Gerasimatos"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicholasg-dev/fixaugment"
  },
  "engines": {
    "vscode": ">=1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
